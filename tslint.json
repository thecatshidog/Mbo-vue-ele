{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "indent": [
            true
        ],
        "interface-name": [
            false
        ],
        "arrow-parens": false,
        "no-unused-expression": false,
        "object-literal-sort-keys": false,
        "no-string-literal": false,
        "one-variable-per-declaration": false
    },
    "rulesDirectory": []
}